{
  "name": "cbt-diagnostic-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/cbt-diagnostic-app.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/cbt-diagnostic-app/issues"
  },
  "homepage": ".",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --open",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:node": "tsc -p tsconfig.node.json --noEmit",
    "lint": "eslint src vite.config.ts --ext .ts,.tsx",
    "lint:fix": "eslint src vite.config.ts --ext .ts,.tsx --fix",
    "format": "prettier . -w",
    "check": "npm run typecheck && npm run typecheck:node && npm run lint && npm run test",
    "deploy:gh": "vite build && gh-pages -d dist"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.26",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.561.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "gh-pages": "^6.2.0",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "postcss": "^8.4.47",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  }
}
